{
    "manifest_version": 3,
    "name": "Web Assist",
    "description": "Web Insights Made Easy!",
    "version": "1.0",
    "action": {
      "default_popup": "index.html"
    },
    "permissions": [
      "activeTab",
      "scripting",
      "storage"
    ],
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["content.css"]
      }
    ]
  }
  